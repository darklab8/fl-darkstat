{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        },
        {
            "name": "Django Parse",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}\\manage.py",
            "args": [
                "runserver"
            ],
            "django": true,
            "env": {
                "DARK_PARSE": "true",
                "DARK_COPY": "true",
                "DARK_SAVE": "true"
            }
        },
        {
            "name": "Django Run",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/manage.py",
            "args": [
                "runserver"
            ],
            "django": true,
            "env": {
                "enable_background": "true"
            }
        },
        {
            "name": "Django Test",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}\\manage.py",
            "args": [
                "test"
            ],
            "django": true
        },
        {
            "name": "Django Copy_Test",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}\\manage.py",
            "args": [
                "test"
            ],
            "django": true,
            "env": {
                "DARK_PARSE": "true",
                "DARK_SAVE": "true",
                "freelancer_folder": "dark_copy"
            }
        },
        {
            "name": "Django Unit",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}\\manage.py",
            "args": [
                "test"
            ],
            "django": true,
            "env": {
                "DARK_LOAD": "true",
            }
        }
    ]
}