{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            // "port": 2345,
            "host": "127.0.0.1",
            "program": "${workspaceFolder}",
            "args": [
                "relay",
            ],
            "env": {
                "ENVERANT_ENV_FILE": "${workspaceFolder}/.vscode/enverant.json"
            },
            "showLog": true
        }
    ]
}